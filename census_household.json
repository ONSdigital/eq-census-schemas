			"answers": [{
				"id": "confirm-establishment-answer",
				"mandatory": false,
				"options": [{
						"label": "Yes, this is a communal establishment",
						"value": "Yes"
					},
					{
						"label": "No, this is a residential home",
						"value": "No, this ia a residential home"
